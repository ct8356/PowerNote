<StackPanel x:Class="PowerNote.EntryClassOptionsPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:PowerNote"
             mc:Ignorable="d"
             DataContext="EntryClassOptionsVM">
    <local:StructurePanel x:Name="StructurePanel" DockPanel.Dock="Top"></local:StructurePanel>
    <local:FilterPanel x:Name="FilterPanel" DockPanel.Dock="Top"></local:FilterPanel>
</StackPanel>
